{
  "name": "babel-plugin-ssr-assets-webpack-loaders",
  "version": "1.0.0-dev",
  "description": "Enable SSR for all assets using webpack loaders",
  "main": "lib/index.js",
  "ava": {
    "files": [
      "src/__tests__/*.spec.js"
    ],
    "require": [
      "@babel/register"
    ],
    "babelrc": "inherit"
  },
  "scripts": {
    "test": "npm run lint && ava -v",
    "build": "babel src/ --out-dir lib/",
    "pre-publish": "npm run build",
    "lint": "eslint src/"
  },
  "license": "MIT",
  "keywords": [
    "babel-plugin",
    "ssr",
    "webpack"
  ],
  "devDependencies": {
